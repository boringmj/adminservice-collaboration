{
    "name": "boringmj/adminservice-collaboration",
    "type": "project",
    "description": "A PHP lightweight fast response framework under development, mainly for the back-end API interface",

    "require": {
        "php": ">=8.0.0",
        "ext-pdo": "*",
        "ext-curl": "*"
    },

    "autoload": {
        "psr-4": {
            "base\\": "AdminService/base/",
            "AdminService\\": "AdminService/core/",
            "AdminService\\model\\": "AdminService/model/",
            "AdminService\\sql\\": "AdminService/sql/",
            "AdminService\\exception\\": "AdminService/exception/",
            "app\\": "AdminService/app/"
        },
        "files": [
            "AdminService/Main.php"
        ]
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "require-dev": {
        "phpunit/phpunit": "10"
    }
}
